{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": ["jest"],
    "rules": {
        "no-underscore-dangle": 0,
        "arrow-body-style": 0,
        "class-methods-use-this": 0,
        "jsx-a11y/label-has-for": 1,
        "linebreak-style": 0,
        "react/jsx-one-expression-per-line": 0
    },
    "env": {
        "jest/globals": true,
        "browser": true
    },
    "globals": {
        "fetch" : true
    }
}